{
  "expo": {
    "name": "Chanced",
    "slug": "Chanced",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chanced.png",
    "scheme": "chancedapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chanced.app",
      "icon": "./assets/images/chanced.png"
    },
    "android": {
      "icon": "./assets/images/chanced.png",
      "edgeToEdgeEnabled": true,
      "package": "com.chanced.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/chanced.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "dark": {
            "image": "./assets/images/splashicon.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
